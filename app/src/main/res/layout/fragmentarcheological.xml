<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:background="#ffffff">

    <ListView
        android:id="@+id/gridviewarcheological"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dip"
         />
    <com.sfuronlabs.ripon.tourbangla.view.ProgressWheel
        android:id="@+id/pwArcheological"
        style="@style/ProgressBarStyle.Reverse"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabArcheological"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:src="@drawable/ic_restore_black_24dp"
        app:borderWidth="0dp"
        app:fabSize="normal" />


</FrameLayout>